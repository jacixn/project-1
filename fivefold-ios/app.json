{
  "expo": {
    "name": "Biblely",
    "slug": "fivefold-ios",
    "version": "1.0.17",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jesusxoi.biblely",
      "buildNumber": "17",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Biblely needs access to your photo library so you can choose a profile picture to personalize your spiritual journey.",
        "NSUserNotificationsUsageDescription": "Biblely sends prayer reminders and achievement notifications to help you maintain your spiritual journey.",
        "BGTaskSchedulerPermittedIdentifiers": [
          "com.jesusxoi.biblely.refresh",
          "com.jesusxoi.biblely.processing"
        ],
        "UIBackgroundModes": [
          "remote-notification",
          "processing"
        ],
        "UIStatusBarHidden": false,
        "UIViewControllerBasedStatusBarAppearance": false,
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#667eea"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.jesusxoi.biblely",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "android.permission.SCHEDULE_EXACT_ALARM"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Faith & Focus Daily - Combine prayer with productivity in a beautiful spiritual companion app",
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#667eea",
          "defaultChannel": "default"
        }
      ],
      "expo-localization",
      "expo-font",
      "expo-dev-client"
    ],
    "extra": {
      "eas": {
        "projectId": "f95f19ac-4387-4307-8812-3202ead7a0e9"
      }
    },
    "owner": "jesusxoi"
  }
}
